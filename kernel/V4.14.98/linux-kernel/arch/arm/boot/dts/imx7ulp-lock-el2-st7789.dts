&lpspi2 {
        #address-cells = <1>;
        #size-cells = <0>;
        pinctrl-names = "default", "sleep";
        pinctrl-0 = <&pinctrl_lpspi2 &pinctrl_lpspi2_cs>;
        cs-gpios = <&gpio_pte 7 GPIO_ACTIVE_LOW>;
        //pinctrl-1 = <&pinctrl_lpspi2>; 
        status = "okay";

        sd2088@0 {
                compatible = "sitronix,st7789v";
                reg = <0>;
                // add pin ctrl
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_st7789v>;

                spi-max-frequency = <24000000>;
                rotate = <0>;
                bgr;
                fps = <15>;
                buswidth = <8>;
                reset-gpios = <&gpio_pte 01 GPIO_ACTIVE_LOW>;
                /*reset-gpios = <&gpio_pte 15 GPIO_ACTIVE_LOW>;*/
                dc-gpios = <&gpio_pte 14 GPIO_ACTIVE_LOW>;
                /*led-gpios = <&gpio_pte 13 GPIO_ACTIVE_HIGH>;*/
                debug = <0>; 
        };
        
};

